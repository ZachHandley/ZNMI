{
  "name": "znmi",
  "version": "0.2.40",
  "description": "A simple and easy TypeScript wrapper around NMI's API",
  "files": [
    "dist"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "dev": "tsx watch src/index.ts",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "deploy": "pnpm run test:run && pnpm run build && npm publish --access public"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/qs": "^6.9.11",
    "@vitest/ui": "^1.6.0",
    "tsup": "^8.0.2",
    "tsx": "^4.7.1",
    "vitest": "^1.2.2"
  },
  "dependencies": {
    "fast-xml-parser": "^4.5.1",
    "ky": "^1.2.0",
    "qs": "^6.11.2",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "zod": "^3.22.4"
  },
  "keywords": [
    "nmi",
    "payment",
    "gateway",
    "typescript",
    "api",
    "znmi",
    "network-merchants"
  ],
  "author": {
    "name": "Zach Handley",
    "url": "https://zachhandley.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ZachHandley/ZNMI"
  },
  "bugs": {
    "url": "https://github.com/ZachHandley/ZNMI/issues"
  },
  "homepage": "https://github.com/ZachHandley/ZNMI#readme",
  "engines": {
    "node": ">=18"
  }
}
