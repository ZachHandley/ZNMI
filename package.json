{
  "name": "znmi",
  "type": "module",
  "version": "0.0.1",
  "description": "A simple and easy TypeScript wrapper around NMI's API",
  "files": [
    "dist"
  ],
  "main": "./dist/znmi.umd.cjs",
  "types": "./dist/index.d.ts",
  "module": "./dist/znmi.js",
  "exports": {
    ".": {
      "import": "./dist/znmi.js",
      "require": "./dist/znmi.umd.cjs"
    }
  },
  "scripts": {
    "build": "vite build && tsc --emitDeclarationOnly",
    "start": "bun index.ts",
    "prepublish": "vitest && bun run build",
    "publish": "vitest && npm publish --access public"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "vitest": "^1.2.2"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@types/qs": "^6.9.11",
    "axios": "^1.6.7",
    "qs": "^6.11.2",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.1.2",
    "zod": "^3.22.4"
  }
}
